{
  "name": "CV-PrinceLoren",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "env-cmd -f environments/.env.${TARGET:-development} next",
    "build": "env-cmd -f environments/.env.${TARGET:-development} next build",
    "start": "next start -p 3000",
    "test": "jest",
    "format": "prettier --write --loglevel silent '*/**/*.{ts,tsx,css,md,json}'",
    "lint:css": "stylelint 'src/**/*.{ts,tsx}'",
    "download-schema": "env-cmd -f environments/.env.${TARGET:-development} yarn helper-ds",
    "generate-types": "env-cmd -f environments/.env.${TARGET:-development} yarn helper-gs",
    "helper-ds": "apollo client:download-schema schema.json",
    "helper-gs": "apollo client:codegen --namespace GTypes --target typescript --globalTypesFile src/@generatedTypes/globalTypes.ts --outputFlat --tsFileExtension d.ts src/@generatedTypes",
    "lint:ts": "eslint --ext .ts,.tsx src",
    "lint": "lint:css && lint:ts",
    "storybook": "start-storybook -p 3003 --ci",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@react-spring/web": "^9.5.5",
    "axios": "^1.1.3",
    "babel-jest": "^29.2.2",
    "babel-plugin-emotion": "^11.0.0",
    "env-cmd": "^10.1.0",
    "express-validator": "^6.14.2",
    "lodash": "^4.17.21",
    "next": "^12.3.1",
    "next-compose-plugins": "^2.2.1",
    "next-connect": "^0.13.0",
    "next-i18next": "^12.1.0",
    "next-plugin-svgr": "^1.1.2",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-toastify": "^9.0.8",
    "react-typed": "^1.2.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "@code-quality/commitlint-config": "^1.1.0",
    "@commitlint/cli": "^17.1.2",
    "@emotion/babel-plugin": "^11.10.2",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.5",
    "@types/react": "18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "csstype": "^3.1.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.0.3",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "ts-jest": "^29.0.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.4",
    "webpack-merge": "^5.8.0"
  }
}
